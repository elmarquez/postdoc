{
  "compilerOptions": {
    "allowSyntheticDefaultImports": true,
    "baseUrl": "./",
    "jsx": "react",
    "lib": ["es2015", "dom"],
    "module": "es2015",
    "moduleResolution": "node",
    "noUnusedLocals": false,
    "pretty": true,
    "resolveJsonModule": true,
    "rootDir": "./",
    "sourceMap": true,
    "target": "es5"
  },
  "exclude": ["node_modules", "**/node_modules/*"]
}
